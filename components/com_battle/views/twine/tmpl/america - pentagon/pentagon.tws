Vhtml {\u000a  box-sizing: border-box;\u000a}\u000a*, *:before, *:after {\u000a  box-sizing: inherit;\u000a}\u000a\u000a\u000a/* Main images in content */\u000aimg{\u000amargin:0 auto 1em;\u000aheight:300px;\u000awidth:500px;\u000adisplay:block;\u000a}\u000a\u000aimg[align=right]{\u000aheight:auto;\u000awidth:auto;\u000amargin:0 0 0 10px\u000a}\u000a\u000a/* Content area */\u000a#passages .body.content {\u000awidth:640px;\u000amargin:0 auto;\u000aposition:relative;\u000apadding: 10px 10px 0;\u000aline-height: 100%;\u000a}\u000a\u000a/* Content area with canvas */\u000a#world {\u000amargin-left: -10px;\u000amargin-right: -10px;\u000a}\u000a\u000a\u000a\u000a\u000a/* Generic Links */\u000a.passage .internalLink:link{\u000afont-weight:bold;\u000acolor:rgb(255,255,255);\u000acolor: #00ffff;\u000a}\u000a.passage .internalLink:visited{\u000acolor:rgb(245,245,245);\u000a}\u000a.passage .internalLink:hover{\u000acolor:rgb(235,235,235);\u000acolor: #009999;\u000a}\u000a.passage .internalLink:focus{\u000acolor:rgb(225,225,225);\u000a}\u000a\u000a\u000a/* Content Styles */\u000a\u000ap {\u000amargin:0 0 1em;\u000aline-height:1.5em;\u000afont-size:1em;\u000a} \u000a\u000a.hotpink {\u000acolor:magenta;\u000aletter-spacing:3px;\u000afont-size:1.5em;\u000a}\u000a\u000a.body.content ul {\u000alist-style:none;\u000a}\u000a\u000a.body.content ul li{\u000amargin-right: 1em;\u000adisplay:inline-block;\u000a}\u000a\u000a\u000a/* buttons positioned overtop of the image */\u000a.touch {\u000aposition:absolute;\u000awidth:500px;\u000aheight:300px;\u000atop:0;\u000a}\u000a\u000a.touch a {\u000aposition:absolute;\u000atop:10%;\u000aleft:20%;\u000a}\u000a\u000a.touch a.internalLink:link{\u000acolor:white;\u000aborder:0 none;\u000apadding:3px 5px;\u000atext-align:center;\u000atext-shadow:2px 2px 2px #000;\u000abackground: rgba(0, 0, 0, 0.7);\u000a}\u000a\u000a.touch a.internalLink:visited{\u000acolor:#009999;\u000aborder:0 none;\u000a}\u000a\u000a.touch a.internalLink:hover{\u000acolor:Chartreuse;\u000a}\u000a\u000a.touch a.internalLink:focus{\u000acolor:DodgerBlue;\u000a}\u000a\u000a.touch a.internalLink:nth-child(1){\u000atop:50%;\u000aleft:70%;\u000a}\u000a\u000a.touch a.internalLink:nth-child(2){\u000atop:80%;\u000aleft:30%;\u000a}\u000a\u000a.touch a.internalLink:nth-child(3){\u000atop:5%;\u000aleft:90%;\u000a}\u000a\u000a\u000a/* Navigation */\u000a.nav ul {\u000amargin:0;\u000apadding:0;\u000a}\u000a\u000a.nav .internalLink:link{\u000abackground:rgba(255, 255, 255, 0.2);\u000acolor:white;\u000aborder:1px solid white;\u000apadding:5px;\u000atext-align:center;\u000amargin:0 2px;\u000aborder-radius:6px;\u000atext-shadow:2px 2px 2px #000;\u000a}\u000a\u000a.nav .internalLink:visited{\u000acolor: #009999;\u000aborder:1px solid #009999;\u000a}\u000a\u000a.nav .internalLink:hover{\u000acolor:Chartreuse;\u000aborder:1px solid Chartreuse;\u000atext-decoration:none;\u000a	-webkit-box-shadow: 0 4px 3px -3px #888;\u000a	   -moz-box-shadow: 0 4px 3px -3px #888;\u000a	        box-shadow: 0 4px 3px -3px #888;\u000a}\u000a\u000a.nav .internalLink:focus{\u000acolor:DodgerBlue;\u000aborder:1px solid DodgerBlue;\u000a}\u000a\u000a\u000a/* Text (including nav etc.) */\u000a.overlay {\u000aposition:absolute;\u000abottom:0;\u000aleft:0;\u000apadding: 0 10px;\u000abackground: rgba(0, 0, 0, 0.7);\u000a}\u000a\u000a.overlay > p:first-child {\u000amargin-top: 1em;\u000a}\u000a\u000a/* Start button  */\u000a.start {\u000atext-align:center;\u000a}\u000a\u000a.start a {\u000adisplay:inline-block;\u000a}\u000a\u000a.start .internalLink:link,\u000a.start .internalLink:visited {\u000abackground:rgba(160, 8, 8, 0.8);\u000acolor:white;\u000aborder:1px solid rgba(160, 8, 8, 1);\u000apadding:5px;\u000atext-align:center;\u000amargin:0 2px;\u000aborder-radius:6px;\u000atext-shadow:2px 2px 2px #000;\u000a}\u000a\u000a.start .internalLink:hover{\u000abackground:rgba(160, 8, 8, 0.9);\u000acolor:white;\u000aborder:1px solid rgba(160, 8, 8, 1);\u000atext-decoration:none;\u000a	-webkit-box-shadow: 0 4px 3px -3px #888;\u000a	   -moz-box-shadow: 0 4px 3px -3px #888;\u000a	        box-shadow: 0 4px 3px -3px #888;\u000a}\u000a\u000a/* Back button  */\u000a.back {\u000adisplay:inline-block;\u000a}\u000a\u000a.back .internalLink:link,\u000a.back .internalLink:visited{\u000abackground:rgba(0, 153, 153, 0.8);\u000acolor:white;\u000aborder:1px solid rgba(0, 153, 153, 1);\u000apadding:5px;\u000atext-align:center;\u000amargin:0 2px;\u000aborder-radius:6px;\u000atext-shadow:2px 2px 2px #000;\u000a}\u000a\u000a.back .internalLink:hover{\u000abackground:rgba(0, 255, 255, 0.9);\u000acolor:white;\u000aborder:1px solid rgba(0, 255, 255, 1);\u000atext-decoration:none;\u000a	-webkit-box-shadow: 0 4px 3px -3px #888;\u000a	   -moz-box-shadow: 0 4px 3px -3px #888;\u000a	        box-shadow: 0 4px 3px -3px #888;\u000a}\u000a\u000a/* Special button  */\u000a.special .internalLink {\u000adisplay:inline-block;\u000a}\u000a\u000a.special .internalLink:link,\u000a.special .internalLink:visited{\u000abackground:rgba(0, 0, 0, 0);\u000acolor: white;\u000aborder:0 solid rgba(100, 100, 100, 1);\u000apadding:5px;\u000atext-align:center;\u000amargin:0 2px;\u000aborder-radius:6px;\u000atext-shadow:2px 2px 2px #000;\u000a-moz-box-shadow:26px 6px 26px #7CFC00 inset;\u000a-webkit-box-shadow:26px 6px 26px #7CFC00 inset;\u000abox-shadow:26px 6px 26px #7CFC00 inset;\u000a}\u000a\u000a.special .internalLink:hover{\u000abackground:rgba(0, 0, 0, 0);\u000acolor:white;\u000aborder:0 solid rgba(100, 100, 100, 1);\u000atext-decoration:none;\u000a-moz-box-shadow:    inset 26px 6px 26px rgba(124,252,0,0.8);\u000a-webkit-box-shadow: inset 26px 6px 26px rgba(124,252,0,0.8);\u000abox-shadow:         inset 26px 6px 26px  rgba(124,252,0,0.8);\u000a}\u000a\u000a\u000a/* status bar */\u000a\u000a.clearfix {\u000aclear: both;\u000a}\u000a \u000a.status-bar div.vitals {\u000a  float: left;\u000a  width: 20%;\u000a  padding: 20px 20px 0 0;\u000a}\u000a\u000a.status-bar .label {\u000a	font-weight: bold;\u000a	line-height: 1;\u000a	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);\u000a	border-radius: 0.25em;\u000a	color: #fff;\u000a	font-size: 75%;\u000a	font-weight: 700;\u000a	white-space: nowrap;\u000a	text-align:left;\u000a	text-transform:uppercase;\u000a	width: 100%;\u000a	display: block;\u000a	margin: 0 0 5px;\u000a}\u000a\u000a.status-bar div.gauge-wrapper {\u000a	position: relative;\u000a	width: 100%;\u000a}\u000a\u000a.status-bar div.gauge {\u000a	background: none repeat scroll 0 0 #0F0F0F;\u000a	border: 1px solid #333;\u000a	border-radius: 25px 25px 25px 25px;\u000a	color: #000000;\u000a	width: 100%;\u000a	float:right;\u000a	margin:5px 0;\u000a	overflow:hidden;\u000a	text-align:right;\u000a	height:22px;\u000a	position: relative;\u000a	padding: 0;\u000a}\u000a\u000a.status-bar div.gauge > div {\u000a	width: 14px;\u000a	height: 20px;\u000a	z-index: 555;\u000a}\u000a\u000a.status-bar div.gauge-wrapper span {\u000a	color:#000;\u000a	background:rgba(255,255,255,0.9);\u000a	border-radius:100%;\u000a	width:22px;\u000a	line-height:22px;\u000a	height:22px;\u000a	position: absolute;\u000a	top: -3px;\u000a	/*left: 0;*/\u000a	z-index: 777;\u000a	font-size: 11px;\u000a	text-align: center;\u000a	margin-left: -11px;\u000a}\u000a\u000a\u000adiv.slack {\u000a	background: none repeat scroll 0 0 rgba(8,8,160,1);\u000a	border-radius: 25px 0 0 25px;\u000a}\u000a\u000adiv.health {\u000a	background: none repeat scroll 0 0 rgba(160,8,8,1);\u000a	border-radius: 25px 0 0 25px;\u000a}\u000a\u000adiv.money {\u000a	background: none repeat scroll 0 0 rgba(8,160,8,1);\u000a	border-radius: 25px 0 0 25px;\u000a}\u000a\u000adiv.mission {\u000a	backgrou